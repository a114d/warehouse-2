{
  "name": "invizio-wms",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Invizio WMS - Production Ready Dolcetto Warehouse Management System by DigiProTech",
  "scripts": {
    "dev": "npx vite",
    "build": "npx vite build",
    "lint": "eslint .",
    "preview": "npx vite preview",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "server:dev": "cd server && npm run dev",
    "server:start": "cd server && npm start",
    "server:migrate": "cd server && npm run migrate",
    "server:install": "cd server && npm install",
    "server:setup": "cd server && npm run setup",
    "full:install": "npm install && npm run server:install",
    "full:dev": "concurrently \"npm run dev\" \"npm run server:dev\"",
    "production:build": "npm run build && cd server && npm install --production",
    "deploy:prepare": "npm run production:build && npm run server:setup"
  },
  "dependencies": {
    "date-fns": "^3.3.1",
    "jspdf": "^2.5.1",
    "jspdf-autotable": "^3.8.2",
    "jsqr": "^1.4.0",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.51.0",
    "react-router-dom": "^6.22.3",
    "recharts": "^2.12.2",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "gh-pages": "^6.1.1",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^7.18.0",
    "vite": "^5.4.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/invizio-wms.git"
  },
  "keywords": [
    "warehouse-management",
    "inventory-system",
    "react",
    "typescript",
    "tailwindcss",
    "wms",
    "inventory-management",
    "supply-chain",
    "postgresql",
    "nodejs",
    "production-ready",
    "barcode-scanner"
  ],
  "author": "DigiProTech",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yourusername/invizio-wms/issues"
  },
  "homepage": "https://invizio-wms.netlify.app"
}
